<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<title>
			{% block title %}Ariane Back-Office
			{% endblock %}
		</title>
		<link rel="stylesheet" href="https://bootswatch.com/5/flatly/bootstrap.min.css"> 
		{% block stylesheets %}{% endblock %}
		<link rel="stylesheet" href="{{ asset('css/app.css') }}">

		</head>
		<body>
		
			<nav class="navbar navbar-expand-lg navbar-dark bg-primary">
				<div class="container-fluid">
					<a class="navbar-brand" href=" {{ path('app_back_user_index') }} ">ARIANE</a>
				
					<button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarColor01" aria-controls="navbarColor01" aria-expanded="false" aria-label="Toggle navigation">
						<span class="navbar-toggler-icon"></span>
					</button>
					<div class="collapse navbar-collapse" id="navbarColor01">
						<ul class="navbar-nav me-auto">
							<li class="nav-item">
								<a class="nav-link" href=" {{ path('app_back_user_index') }} ">Utilisateurs</a>
							</li>
							{# <li class="nav-item">
								<a class="nav-link" href=" {{ path('app_back_user_administrator_list') }} ">Administration</a>
							</li>
							<li class="nav-item">
								<a class="nav-link" href=" {{ path('app_back_user_association_list') }} ">Associations</a>
							</li>
							<li class="nav-item">
								<a class="nav-link" href=" {{ path('app_back_user_volunteer_list') }} ">Volontaires</a>
							</li> #}
							<li class="nav-item">
								<a class="nav-link" href=" {{ path('app_back_post_index') }} ">Annonces</a>
							</li>
							<li class="nav-item">
								<a class="nav-link" href=" {{ path('app_back_candidacy_index') }} ">Candidatures</a>
							</li>
							<li class="nav-item">
								<a class="nav-link" href=" {{ path('app_back_supply_index') }} ">Fournitures</a>
							</li>
							<li class="nav-item">
								<a class="nav-link" href=" {{ path('app_back_category_index') }} ">Catégories</a>
							</li>
							{% if not app.user %}
								<li class="nav-item">
									<a class="nav-link" href="#">Connexion</a>
								</li>
							{% else %}
								<li class="nav-item">
									<a class="nav-link" href=" {{path('app_logout')}} ">Déconnexion</a>
								</li>
							{% endif %}
						</ul>
					</div>
				</div>
			</nav>
			<div class="container"> {% block body %}{% endblock %}
				</div>
				{% block javascript %}{{ encore_entry_script_tags('app') }}{% endblock %}
				<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
			</body>
		</html>
